{
  "name": "gem-finder",
  "version": "1.0.0",
  "description": "Find diamonds in Block Games using seed analysis",
  "main": "gem-finder.js",
  "type": "commonjs",
  "scripts": {
    "start": "ts-node --esm ore-finder.ts",
    "build": "tsc",
    "find-diamonds": "ts-node --esm minecraft-diamond-finder.ts",
    "find-gold": "ts-node --esm minecraft-gold-finder.ts",
    "find-all": "./find-all-ores.sh"
  },
  "dependencies": {
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "keywords": [
    "block-game",
    "diamonds",
    "seed",
    "world-generation"
  ],
  "author": "Johannes Lockhead Koch",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.7"
  }
}