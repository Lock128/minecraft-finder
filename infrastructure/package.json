{
  "name": "minecraft-finder-web-hosting",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for hosting Minecraft Ore Finder web application",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "validate-config": "ts-node scripts/validate-config.ts",
    "validate-config:dev": "ts-node scripts/validate-config.ts dev",
    "validate-config:staging": "ts-node scripts/validate-config.ts staging",
    "validate-config:prod": "ts-node scripts/validate-config.ts prod",
    "validate-config:verbose": "ts-node scripts/validate-config.ts --verbose",
    "validate-config:report": "ts-node scripts/validate-config.ts --output validation-report.txt",
    "security:audit": "npm audit --audit-level=high",
    "security:scan": "npm run security:audit && npm run security:validate",
    "security:validate": "npm run validate-iam && npm run validate-security-config",
    "security:check": "ts-node scripts/security-check.ts",
    "security:check:dev": "ts-node scripts/security-check.ts dev",
    "security:check:staging": "ts-node scripts/security-check.ts staging",
    "security:check:prod": "ts-node scripts/security-check.ts prod",
    "security:check:verbose": "ts-node scripts/security-check.ts dev --verbose",
    "validate-iam": "echo 'IAM validation - implement with cdk-iam-floyd if needed'",
    "validate-security-config": "ts-node -e \"import { SecurityConfig } from './src/utils/security-config'; console.log('Security config validation passed');\""
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.120.0",
    "constructs": "^10.3.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "typescript",
    "flutter",
    "web-hosting",
    "cloudfront",
    "s3"
  ],
  "author": "Minecraft Ore Finder Team",
  "license": "MIT"
}
