{
  "environment": "staging",
  "environmentConfig": {
    "name": "staging",
    "description": "Staging environment for pre-production testing",
    "isProduction": false,
    "allowedRegions": ["us-east-1"],
    "featureFlags": {
      "enableAdvancedMonitoring": true,
      "enableCostOptimization": true,
      "enableSecurityHardening": true,
      "enablePerformanceOptimization": true
    },
    "limits": {
      "maxCacheTtl": 604800,
      "maxRumSamplingRate": 1.0,
      "maxS3LifecycleDays": 180
    }
  },
  "domainConfig": {
    "domainName": "staging-minecraft.lockhead.cloud",
    "hostedZoneId": "REPLACE_WITH_HOSTED_ZONE_ID",
    "crossAccountRoleArn": "REPLACE_WITH_CROSS_ACCOUNT_ROLE_ARN",
    "certificateRegion": "us-east-1"
  },
  "monitoringConfig": {
    "rumAppName": "minecraft-finder-staging",
    "samplingRate": 0.5,
    "enabledMetrics": [
      "PerformanceNavigationTiming",
      "PerformanceResourceTiming",
      "LargestContentfulPaint",
      "FirstInputDelay",
      "CumulativeLayoutShift",
      "JsError",
      "HttpError"
    ],
    "enableExtendedMetrics": true
  },
  "cachingConfig": {
    "defaultTtl": 86400,
    "maxTtl": 604800,
    "staticAssetsTtl": 604800,
    "htmlTtl": 1800
  },
  "s3Config": {
    "bucketNamePrefix": "minecraft-finder-web-staging",
    "versioning": true,
    "publicReadAccess": false,
    "lifecycleRules": [
      {
        "id": "DeleteOldVersions",
        "enabled": true,
        "transitionToIADays": 30,
        "expirationDays": 180,
        "abortIncompleteMultipartUploadDays": 7
      }
    ]
  },
  "resourceNaming": {
    "resourcePrefix": "minecraft-finder",
    "resourceSuffix": "staging",
    "includeEnvironment": true,
    "includeRandomSuffix": true,
    "customPatterns": {
      "s3Bucket": "minecraft-finder-web-staging-{random}",
      "cloudFrontDistribution": "minecraft-finder-cf-staging",
      "certificate": "minecraft-finder-cert-staging",
      "rumApplication": "minecraft-finder-rum-staging"
    }
  },
  "costAllocation": {
    "costCenter": "Engineering-Staging",
    "projectCode": "MINECRAFT-FINDER-STAGING",
    "department": "Engineering",
    "budgetThreshold": 100,
    "enableDetailedBilling": true,
    "customCostTags": {
      "BillingGroup": "PreProduction",
      "Owner": "QATeam",
      "Purpose": "Testing"
    }
  },
  "tags": {
    "Project": "MinecraftOreFinder",
    "Environment": "staging",
    "ManagedBy": "CDK",
    "CostCenter": "Engineering-Staging",
    "ProjectCode": "MINECRAFT-FINDER-STAGING",
    "Department": "Engineering",
    "BillingGroup": "PreProduction",
    "Owner": "QATeam",
    "Purpose": "Testing",
    "Tier": "Staging",
    "DataClassification": "Internal",
    "BackupRequired": "true",
    "MonitoringLevel": "Enhanced"
  }
}